@{
    Layout = "~/_SiteLayout.cshtml";
    Page.Title = "Home";
}

@section Styles {
    <style> /*-- cover-img --*/
        .page-img {background: url('/images/body-bg.jpg') fixed; background-size: cover;}
        .cover-img {background-image: url('/images/pegasus_sun_rev.jpg'); background-size: cover;}
        
        .wrapper-inner{
            background: url('/images/footer-bg.jpg') top center;
            background-size: cover;
            background-repeat: no-repeat;
            
            border-left: 2px solid #000;
            border-right: 2px solid #000;
        }
        
    </style>
}

@section featured {

    <p><!-- Page headlines go here.--></p>

    <p>Authenticated: @WebSecurity.IsAuthenticated</p>

    <section id="login">
        @if (WebSecurity.IsAuthenticated) {
            <text>
                Hello, <a class="email" href="~/Account/Manage" title="Manage">@WebSecurity.CurrentUserName</a>!
                <form id="logoutForm" action="~/Account/Logout" method="post">
                    @AntiForgery.GetHtml()
                    <a href="javascript:document.getElementById('logoutForm').submit()">Log out</a>
                </form>
            </text>
        } else {
            <ul>
                <li><a href="~/Account/Register">Register</a></li>
                <li><a href="~/Account/Login">Log in</a></li>
            </ul>
        }
    </section>

}

